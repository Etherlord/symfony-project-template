{
    "name": "symfony-template/api",
    "description": "Backend App",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "baldinof/roadrunner-bundle": "^3.3.2",
        "cuyz/valinor": "^2.3.2",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-migrations-bundle": "^3.7.0",
        "doctrine/migrations": "^3.9.6",
        "lexik/jwt-authentication-bundle": "^3.2.0",
        "nelmio/api-doc-bundle": "^5.9.3",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^1.33",
        "ramsey/uuid": "^4.9.2",
        "symfony/asset": "^7.4.4",
        "symfony/cache-contracts": "^3.6.0",
        "symfony/console": "^7.4.4",
        "symfony/dependency-injection": "^7.4.5",
        "symfony/dotenv": "^7.4.0",
        "symfony/event-dispatcher": "^7.4.4",
        "symfony/flex": "^2.10.0",
        "symfony/framework-bundle": "^7.4.5",
        "symfony/http-foundation": "^7.4.5",
        "symfony/http-kernel": "^7.4.5",
        "symfony/lock": "^7.4.5",
        "symfony/messenger": "^7.4.4",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/property-access": "^7.4.4",
        "symfony/property-info": "^7.4.5",
        "symfony/rate-limiter": "^7.4.5",
        "symfony/routing": "^7.4.4",
        "symfony/runtime": "^7.4.1",
        "symfony/serializer": "^7.4.5",
        "symfony/twig-bundle": "^7.4.4",
        "symfony/validator": "^7.4.5",
        "symfony/yaml": "^7.4.1",
        "thesis/postgres": "dev-master",
        "thesis/thesis": "dev-master",
        "twig/twig": "^3.23.0",
        "zircote/swagger-php": "^4.11.1"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.9.1",
        "deptrac/deptrac": "^4.6.0",
        "ergebnis/composer-normalize": "^2.50",
        "friendsofphp/php-cs-fixer": "^3.94.0",
        "icanhazstring/composer-unused": "^0.9.6",
        "maglnet/composer-require-checker": "^4.20.0",
        "phpunit/phpunit": "^12.5.11",
        "phpyh/coding-standard": "^3.0.1",
        "rector/rector": "^2.3.6"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Etherlord/postgres-thesis"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Etherlord/thesis"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/Infrastructure/functions.php",
            "src/Infrastructure/DependencyInjection/functions.php",
            "src/Infrastructure/Uuid/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": [
                "src-dev/",
                "tests/"
            ]
        },
        "files": [
            "src-dev/Tests/Functional/functions.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "audit": {
            "abandoned": "ignore"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
